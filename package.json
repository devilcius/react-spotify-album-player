    "@rollup/plugin-node-resolve": "^13.3.0",    
    "@svgr/rollup": "^8.0.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
